<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <h2 class="text-center">My Wikis</h2>
    <table class="table table-striped table-bordered">
      <%= render @wikis %>
    </table>

    <div class="text-center">
      <% if current_user %>
        <%= link_to "New Wiki", new_wiki_path, class: 'btn btn-success' %>
      <% end %>
    </div>
  </div>
</div>
